vetor = list()
t = int(input()) #declara valor
cont = 0

for c in range(1000): #preenchimento de vetor
    for i in range(0, t): #sequencia dos valores preenchidos
        vetor.append(i)

for i in range(len(vetor)): #condição para que o vetor termine em "T-1"
    if i == 1000:
        break
    print(f'N[{i}] = {vetor[i]}')
