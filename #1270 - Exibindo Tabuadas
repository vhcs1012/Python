n1 = int(input())
n2 = int(input())

cont = 0
if n1 < n2:
    for c in range(n1, n2 + 1):
        for cont in range(1, 11):
            res = c * cont
            print(f'{c} x {cont} = {res}')
            cont += 1
        print('----------')

elif n1 == n2:
    for cont in range(1, 11):
        res = n1 * cont
        print(f'{n1} x {cont} = {res}')
        cont += 1
    print('----------')
else:
    print('Nenhuma tabuada no intervalo!')
